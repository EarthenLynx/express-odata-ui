<mvc:View controllerName="sap.ui.mgmt.odata.routes.controller.Logs" displayBlock="true" 
    xmlns="sap.m" 
    xmlns:l="sap.ui.layout" 
    xmlns:mvc="sap.ui.core.mvc">

    <l:BlockLayout id="BlockLayout" background="Dashboard">
        <l:BlockLayoutRow accentCells="Accent1">
            <l:BlockLayoutCell title="Error logs">
                <mvc:XMLView viewName="sap.ui.mgmt.odata.routes.view.logs.Error" />
            </l:BlockLayoutCell>
            <l:BlockLayoutCell title="Info logs">
                <mvc:XMLView viewName="sap.ui.mgmt.odata.routes.view.Logs.Info"/>
            </l:BlockLayoutCell>
        </l:BlockLayoutRow>
    </l:BlockLayout>
</mvc:View>