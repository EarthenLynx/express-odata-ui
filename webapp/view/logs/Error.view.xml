<mvc:View controllerName="sap.ui.mgmt.odata.routes.controller.Logs" displayBlock="true" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core">
	<Table id="error-log" inset="false" items="{
			path: 'errorLogs>/'
		}">
		<columns>
			<Column width="4em">
				<Text text="Type" />
			</Column>
			<Column width="8em">
				<Text text="Time" />
			</Column>
			<Column>
				<Text text="Error" />
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<ObjectIdentifier text="{errorLogs>type}"/>
					<ObjectIdentifier text="{errorLogs>time}"/>
					<ObjectIdentifier text="{errorLogs>msg}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>

</mvc:View>