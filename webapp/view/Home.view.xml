<mvc:View controllerName="sap.ui.mgmt.odata.routes.controller.Home" displayBlock="true" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="home-page" title="Home">
		<content>
			<mvc:XMLView viewName="sap.ui.mgmt.odata.routes.view.components.Logs"/>
			<mvc:XMLView viewName="sap.ui.mgmt.odata.routes.view.components.Codecheck"/>
		</content>
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button press="handleNavToNewRoute" text="Create new Route" type="Accept" icon="sap-icon://add" />
			</OverflowToolbar>
		</footer>
	</Page>

</mvc:View>