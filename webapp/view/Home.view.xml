<mvc:View controllerName="sap.ui.mgmt.odata.routes.controller.Home" displayBlock="true" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="page" title="{i18n>title}">
		<content>
			<mvc:XMLView viewName="sap.ui.mgmt.odata.routes.view.Logs"/>
			<mvc:XMLView viewName="sap.ui.mgmt.odata.routes.view.Codecheck"/>
		</content>
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button press="handleOpenPopup" text="Create new Route" type="Accept" icon="sap-icon://add" />
			</OverflowToolbar>
		</footer>
	</Page>

</mvc:View>